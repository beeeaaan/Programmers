class Solution {
    public double solution(int[] numbers) {
        double answer = 0;
        double sum = 0;
        
        // 1. 배열의 전체 합 구하기
        for(int i = 0; i <numbers.length; i++) {
            sum += numbers[i];
        }
        // 2. 평균 구하기
        answer = sum / numbers.length;
        return answer;
    }
}
