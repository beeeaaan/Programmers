class Solution {
    public int[] solution(int[] num_list) {
        int[] answer = new int[num_list.length];
        
        //
        for(int i = 0; i < num_list.length; i++) {. // 인덱스 번호
            answer[i] = num_list[num_list.length - i - 1];  // 인덱스 안의 숫자
        }
        return answer;
    }
}
