class Solution {
    public int solution(String[] s1, String[] s2) {
        int answer = 0;
        
        for(int i = 0; i < s1.length; i++) { // s1 배열
            for(int j = 0; j < s2.length; j++) { //s2 배열
                if(s1[i].equals(s2[j])) { // 문자열 일치 여부 확인
                    answer++; // 일치 갯수 세기
                }
            }
        }
        return answer;
    }
}
