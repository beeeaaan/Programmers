class Solution {
    public int solution(int[] numbers) {
        int answer = 45;  // 1 ~ 45 까지의 합
        
        // for(object:List) : ':' 을 중심으로 오른쪽 리스트의 인덱스를 하나씩 순차적으로 접근하여 왼쪽에 있는 변수에 저장하겠다
        for(int number : numbers) {
            answer -= number;
        }
        return answer;
    }
}
