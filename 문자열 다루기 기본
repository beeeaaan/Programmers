class Solution {
  public boolean solution(String s) {
      if(s.length() == 4 || s.length() == 6){
          try {int x = Integer.parseInt(s); // 숫자로 변환시
              return true;
               // NumberFormatException : 문자열이 나타내는 숫자와 일치하지 않는 타입의 숫자로 변환시 발생
          } catch(NumberFormatException e){ 
              return false;
          }
      }
      else return false;
  }
}
