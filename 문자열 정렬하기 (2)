import java.util.Arrays;

class Solution {
    public String solution(String my_string) {
        String answer = "";
        
        my_string = my_string.toLowerCase(); // 소문자 만들기
        char[] stringChar = my_string.toCharArray(); // 문자열 한 글자씩 나눠 배열 넣기
        Arrays.sort(stringChar);  // 오름차순 정렬
        answer = new String(stringChar);
        return answer;
    }
}
