class Solution {
    public String solution(String my_string) {
        String answer = "";
        
        // 1. 문자열 뒤부터 실행하기
        for(int i = my_string.length()-1; i >= 0; i--) {
        // 2. substring로 문자열 자르기
            answer += my_string.substring(i, i+1);
        }
        return answer;
    }
}
