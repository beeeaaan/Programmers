class Solution {
    public String solution(String my_string, int n) {
        String answer = "";
        
        for(int i = 0; i <= my_string.length()-1; i++) { // 문자 뽑기
            for(int j = 0; j <= n - 1; j++) { // 반복횟수 정하기
                answer += my_string.charAt(i); // 특정 문자 반복하기
            }
        }
        return answer;
    }
}
