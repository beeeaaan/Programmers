class Solution {
    public int solution(int n) {
        int answer = 0;
        
        for(int i = 1; i <= n; i++) {
            int count = 0;  // 약수의 개수
            
            for(int j = 1; j <= i; j++) {
                count += (i % j == 0) ? 1 : 0;  // 약수 확인
            }
            answer += (count >= 3) ? 1 : 0;  // 합성수 확인
        }
        return answer;  // 합성수의 개수
    }
}
