class Solution {
    public long solution(int a, int b) {
        long answer = 0;
        
        if(a < b) { // a가 b보다 작을 경우
            for(int i = a; i <= b; i++) {
                answer += i;
            }
        } else { // b가 a보다 작을 경우
            for(int i = b; i <= a; i++) {
                answer += i;
            }
        }
        return answer;
    }
}
